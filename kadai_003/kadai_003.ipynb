        "import numpy as np\n",
        "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "Seki=np.dot(A, B)\n",
        "M=np.dot(A, B).max()\n",
        "print(\"行列の積:\", Seki)\n",
        "print(\"行の要素の最大値:\", M)"
